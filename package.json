  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "npm-run-all": "^4.1.5",
